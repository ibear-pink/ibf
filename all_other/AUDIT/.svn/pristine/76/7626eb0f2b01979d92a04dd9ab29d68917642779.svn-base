	<select id="SEL_BILLDAY_DB" parameterClass="Map" resultNum="6" db_no="0">select CONTRACT_NO,ID_NO,BILL_DAY,EFF_DATE,EXP_DATE,DUR_FLAG from UR_BILLDAY_INFO where ID_NO=${ID_NO|long} $E[and EFF_DATE=${EFF_DATE|char}] and exp_date @gt= to_date(to_char(add_months(sysdate,-1),'yyyymm')||'01000000','yyyymmddhh24miss')</select>
