	getCurDateTime(ym,"yyyymm");
	m_Ibear = br;
	m_CfgInfo = pServerCfg;
	m_logFile = m_CfgInfo->logFile;
	strcpy(m_DataType,processName);
	printf("outdir=%s\n",m_CfgInfo->out_cfg_info.outdir);
