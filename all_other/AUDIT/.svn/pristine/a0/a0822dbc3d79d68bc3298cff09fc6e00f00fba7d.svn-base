	<select id="SEL_UGM_DB_1" parameterClass="Map" resultNum="11" db_no="0">select a.GROUP_ID,b.id_no,b.GROUP_TYPE,b.GROUP_CODE,a.MEMBER_ROLE_ID,a.MEMBER_ID,a.OBJECT_TYPE,a.OBJECT_ID,c.phone_NO,a.EFF_DATE,a.EXP_DATE from UR_USERGROUPMBR_INFO a,UR_USERGROUP_INFO b,UR_USER_INFO c where a.group_id=b.group_id and a.object_id=c.id_no and a.MEMBER_ID=${MEMBER_ID|long} $E[and a.OBJECT_ID=${OBJECT_ID|char}] and object_type='1' and a.exp_date @gt= to_date(to_char(add_months(sysdate,-1),'yyyymm')||'01000000','yyyymmddhh24miss')</select>
	<select id="SEL_UGM_DB_0" parameterClass="Map" resultNum="11" db_no="0">select a.GROUP_ID,b.id_no,b.GROUP_TYPE,b.GROUP_CODE,a.MEMBER_ROLE_ID,a.MEMBER_ID,a.OBJECT_TYPE,a.OBJECT_ID,a.object_id,a.EFF_DATE,a.EXP_DATE from UR_USERGROUPMBR_INFO a,UR_USERGROUP_INFO b where a.group_id=b.group_id and a.MEMBER_ID=${MEMBER_ID|long} $E[and a.OBJECT_ID=${OBJECT_ID|char}] and object_type!='1' and a.exp_date @gt= to_date(to_char(add_months(sysdate,-1),'yyyymm')||'01000000','yyyymmddhh24miss')</select>
