	<select id="SEL_USER_BDS" parameterClass="Map" resultNum="14" db_no="1">select ID_NO,CONTRACT_NO,PHONE_NO,GROUP_FLAG,MASTER_SERV_ID,PRE_RUN_CODE,RUN_CODE,RUN_TIME,CREATE_DATE,OPEN_TIME,FIRST_USEDATE,COMPLETED_DATE,BILL_START_TIME,OWNER_TYPE from BDS_USER_INFO where PHONE_NO=${PHONE_NO|char} $E[and ID_NO=${ID_NO|long}]</select>
	<select id="SEL_USER_DB" parameterClass="Map" resultNum="14" db_no="0">select ID_NO,CONTRACT_NO,PHONE_NO,GROUP_FLAG,MASTER_SERV_ID,OLD_RUN,RUN_CODE,RUN_TIME,CREATE_DATE,OPEN_TIME,FIRST_USEDATE,COMPLETED_DATE,BILL_START_TIME,OWNER_TYPE from UR_USER_INFO where PHONE_NO=${PHONE_NO|char} $E[and ID_NO=${ID_NO|long}]</select>
