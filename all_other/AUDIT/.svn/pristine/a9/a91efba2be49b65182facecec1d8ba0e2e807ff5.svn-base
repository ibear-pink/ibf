
[SYSTEM]
#程序名称
program_name=CHKBDS
#连接的数据库个数
db_num=2
db_id_0=1 #物理库billcolldev
db_id_1=2 #内存库billing_data
#采用DSCI连接数据库,请配置${DSCI_PATH}/cfg/db.cfg

[SERVER_INFO]
[PROC_SERVER]
#执行方式：1.实时接口, 2.定时接口 3.自定义接口
deal_type=3
#是否需要数据分发：1，需要；0，不需要
is_give=0
#接口类型：1 文件接口，2 接口表
interface_type=2
#对应日志服务器名称
server_name=CHKBDS
#接口表模式数据备份方式
#备份模式（1：文件方式，2：表方式）
tab_type=0

[LOG_INFO]
log_num=1
[LOG]
#日志服务器路径
log_path=/ocsapp/account/pkg/src/cpp/AUDIT/log
#日志服务器名称
log_name=CHKBDS
#log_thread_num=0时,各线程共享一个日志
log_thread_num=1
#日志服务器包含日志文件数目
log_file_num=3
#日志服务器类型，1000,1001,1002,1003,1004（日志类型以","进行分割)
#1000:通用日志，1001:运行日志，1002:告警日志，1003:错误日志，1004:统计日志

log_type1=1003
#日志备份间隔时间（单位:秒）
log_intervaltime1=0
#日志备份大小（单位为：M）
log_backupsize1=0
#日志处理模式（1：同步方式，2：异步方式）
log_procmode1=2
#日志备份类型（1：日期，2：文件大小，3：时间间隔，4：小时）
log_backuptype1=1

log_type2=1001
#日志备份间隔时间（单位:秒）
log_intervaltime2=0
#日志备份大小（单位为：M）
log_backupsize2=0
#日志处理模式（1：同步方式，2：异步方式）
log_procmode2=2
#日志备份类型（1：日期，2：文件大小，3：时间间隔，4：小时）
log_backuptype2=1

log_type3=1000
#日志备份间隔时间（单位:秒）
log_intervaltime3=0
#日志备份大小（单位为：M）
log_backupsize3=0
#日志处理模式（1：同步方式，2：异步方式）
log_procmode3=2
#日志备份类型（1：日期，2：文件大小，3：时间间隔，4：小时）
log_backuptype3=1


